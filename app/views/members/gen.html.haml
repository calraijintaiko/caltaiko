.generation(itemscope itemtype="http://schema.org/Organization")
  %header
    %h2 GENERATION #{@gen}

  %span.schema-info(itemprop="name")= "Generation #{@gen}"

  - if @members.length > 0
    = render "current_members", members: @members
  - else
    %p(id="nice-try-wise-guy")= "Unfortunately, there are not yet any members of Generation #{@gen}. Please check back again in #{2004 + @gen.to_i}!"
